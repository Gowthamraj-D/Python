class Solution(object):
    def findMin(self, a):
        """
        :type nums: List[int]
        :rtype: int
        """
        l=0
        r=len(a)-1
        while l<=r:
            m=(l+r)//2
            if (a[l]==a[m]) and (a[r]==a[m]):
                l+=1
                r-=1
                continue
            if a[m]<=a[r]:
                r=m
            else:
                l=m+1
        return a[m]
            
